{
    "name": "vikbert/cloud-cron-bundle",
    "description": "A lightweight and super fast symfony bundle to schedule cron jobs in cloud environment",
    "type": "symfony-bundle",
    "version": "0.1.0",
    "require": {
        "php": ">=7.4",
        "doctrine/annotations": "^1.13",
        "adhocore/cron-expr": "^1.1",
        "doctrine/orm": "^2.10",
        "symfony/console": "^5.3",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "ext-json": "*"
    },
    "license": "Apache",
    "autoload": {
        "psr-4": {
            "Vikbert\\CloudCronBundle\\": "src/"
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/vikbert/CloudCronBundle.git"
    }
    ],
    "extra": {
        "symfony": {
            "require": "5.0.*"
        }
    },
    "authors": [
        {
            "name": "vikbert",
            "email": "segentor@gmail.com"
        }
    ]
}
