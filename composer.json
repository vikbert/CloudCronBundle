{
    "name": "chapterphp/cloud-cron-bundle",
    "archive": {
        "name": "cloud-cron-bundle",
        "exclude": ["example", "docs"]
    },
    "description": "A lightweight and super fast symfony bundle to schedule cron jobs in cloud environment",
    "version": "1.0.0",
    "keywords": ["stackit", "cron", "cron job", "process", "bundle"],
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.4",
        "doctrine/annotations": "^1.13",
        "adhocore/cron-expr": "^1.1",
        "doctrine/orm": "^2.10",
        "symfony/console": "^5.3",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "ext-json": "*"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Chapterphp\\CloudCronBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Xun Zhou",
            "email": "xun.zhou@mail.schwarz"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://schwarzit.jfrog.io/schwarzit/api/composer/de-sit-phpc-php-local"
        },
        {
            "packagist": false
        }
    ]
}
